  Lead_id,
    Assign_To_MR_Main_Agent_Name,
    Remarks_History,
    SQV_Lead_Intent,
    Campaign_NAME,
    call_Count,
    agent_Id,
    latest_called_at,
    customer_Name,
    Alt_Phone,
    Address,
    City,
    State,
    Postal_Code,
    Province,
    date_Of_Birth,
    latest_recording_url,
    Disposition,
    Lead_Id_From_Dialer

FROM [dbo].[DialShree-Kairali-Received API];


