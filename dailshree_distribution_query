    [customer_Name] VARCHAR(MAX) NULL,
    [Alt_Phone] VARCHAR(MAX) NULL,
    [Address] VARCHAR(MAX) NULL,
    [City] VARCHAR(MAX) NULL,
    [State] VARCHAR(MAX) NULL,
    [Postal_Code] VARCHAR(MAX) NULL,
    [Province] VARCHAR(MAX) NULL,
